<h1>Add a Picture</h1>

<div class="add_pic_form">
  <%= form_for(@picture) do |f| %>
  <div class="field">
    <div>
      <%= f.label :artist %><br>
      <%= f.text_field :artist %>
    </div>
    <div class="field">
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </div>

    <div class="field">
      <%= f.label :url %><br>
      <%= f.text_field :url %>
    </div>

    <div class="field">
      <%= f.submit "Save" %>
    </div>
  <% end %>
</div>

<%= link_to("Back to Pictures", "/pictures") %>
